{
  "framework": "vite",
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "routes": [
    { 
      "src": "/assets/(.*)\\.js", 
      "headers": { "content-type": "application/javascript" },
      "continue": true
    },
    { 
      "src": "/assets/(.*)\\.css", 
      "headers": { "content-type": "text/css" },
      "continue": true
    },
    { "src": "/(.*)", "dest": "/index.html" }
  ],
  "github": {
    "silent": true
  }
}
